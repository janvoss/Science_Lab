@article{vopel_unmenschliche_2023,
	title = {Die „unmenschliche“ Revolution – Künstliche Intelligenz als Schicksalstechnologie für Deutschland und Europa},
	volume = {103},
	url = {https://www.wirtschaftsdienst.eu/inhalt/jahr/2023/heft/8/beitrag/die-unmenschliche-revolution-kuenstliche-intelligenz-als-schicksalstechnologie-fuer-deutschland-und-europa.html},
	abstract = {Künstliche Intelligenz ({KI}) hat Menschen seit jeher gleichermaßen zu utopischen wie dystopischen Visionen inspiriert. Doch sie wird die Menschheit weder erlösen noch auslöschen. Das Gute und das Böse sind Grundtatbestände der Welt, sie haben schon das Feuer wahlweise zu einem Werkzeug oder einer Waffe gemacht. So wird es auch bei {KI} sein.},
